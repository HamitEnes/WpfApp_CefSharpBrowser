<Window x:Class="WpfApp_CefSharpBrowser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp_CefSharpBrowser"
        xmlns:cef="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1024">
    <Grid>
        <cef:ChromiumWebBrowser x:Name="Browser_Sample" Address="https://www.google.com" FrameLoadEnd="Browser_Sample_FrameLoadEnd" JavascriptMessageReceived="Browser_Sample_JavascriptMessageReceived" />
    </Grid>
</Window>
